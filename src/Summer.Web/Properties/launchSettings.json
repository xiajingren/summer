{
  "profiles": {
    "ServerClient": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "LAUNCH_MODE": "ServerClient",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Server": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "LAUNCH_MODE": "Server",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "publishAllPorts": true
    }
  }
}